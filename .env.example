# --- Required: set your own User-Agent for MET Norway ---
PROVIDER_USER_AGENT=YourName-WeatherProject/1.0 (contact: your@email)

# Mongo (container network)
MONGODB_URI=mongodb://mongo:27017/weather
DB_NAME=weather

# Location (Bangkok by default)
LAT=13.7563
LON=100.5018
PLACE_NAME=Bangkok, TH
TIMEZONE=Asia/Bangkok

# Fetcher
FETCH_INTERVAL_MINUTES=10
RUN_ONCE=false
